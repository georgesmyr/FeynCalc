 RowBox[{
  RowBox[{
   RowBox[{"SP", "[", "p1", "]"}], "=", 
   SuperscriptBox["m", "2"]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SP", "[", "p2", "]"}], "=", 
   SuperscriptBox["m", "2"]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
